{
    "compilerOptions": {
        "target": "es2022",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": false,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "react-jsx",
        "jsxImportSource": "woby",
        "incremental": true
    },
    "include": [
        "next-env.d.ts",
        "types/*.d.ts",
        "**/*.ts",
        "**/*.tsx",
        "**/**/*.ts",
        "**/**/*.tsx",
        "../**/*.ts",
        "../**/*.tsx"
    ],
    "exclude": [
        "node_modules"
    ]
}